{
  "name": "vvui",
  "version": "1.1.0",
  "description": "This is a UI components for Vue 2.x ",
  "author": "monkey-wang <2424880409@qq.com>",
  "main": "packages/index.js",
  "private": false,
  "scripts": {
    "tag": "node build/tag.js",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "dev": "cross-env NODE_ENV=development  webpack-dev-server --config build/webpack.dev.conf.js",
    "dev:dev": "cross-env NODE_ENV=development  webpack-dev-server --config build/webpack.dev.conf.js",
    "dev:prod": "cross-env NODE_ENV=production  webpack-dev-server --config build/webpack.prod.conf.js --open",
    "start": "npm run dev",
    "build:md": "rimraf dist && node build/build.js",
    "build:lib": "npm run clean && npm run build:lib-a && npm run build:lib-css",
    "build:lib-a": "npm run clean && webpack --config ./build/webpack.lib.common && webpack --config ./build/webpack.lib.compontent",
    "build:lib-t": "webpack --config ./build/webpack.lib.compontent",
    "build:lib-c": "npm run clean && webpack --config ./build/webpack.lib.common",
    "build:lib-css": "node build/bin/css.js && gulp build --gulpfile packages/theme-default/gulpfile.js && cp-cli packages/theme-default/lib lib/theme-default",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs"
  },
  "dependencies": {
    "markdown-it-container": "^2.0.0",
    "moment": "^2.19.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.25.0",
